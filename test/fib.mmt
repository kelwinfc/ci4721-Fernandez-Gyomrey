fun int fibIter(int n){
    if ( n == 0 ){
        return 0;
    } elif ( n == 1 ){
        return 1;
    } else {
        
        int fPrev = 0;
        int fCurr = fPrev+1;
        int i=1;
        
        while ( i < n ){
            int tmp = fPrev;
            fPrev = fCurr;
            fCurr = tmp + fCurr;
            i = i+1;
        }
        
        return fCurr;
        
    }
    
}

